  <div id="resume" class="sectionhead">
    EDUCATION
  </div>
  {{#each schools as |school|}}
  <div class="school">
    <div class="left" id="logo">
      <img src="{{school.image}}" width="100%" />
    </div>
    <div class="right" id="parent">
      <div class="rightinfo" id="child">
        <div class="name">
          {{school.name}}
        </div>
        <div class="date">
          {{school.time}}
        </div>
        <div class="information">
          {{school.description}}
        </div>
      </div>
    </div>
  </div>
  {{/each}}
</div>
